<html>
<link rel="stylesheet" type="text/css" href="/stylesheets/users_list.css" />
<body>
<div class="table-users">
  <div class="container">
    <div class="header">Lista de usuários cadastrados</div>

      <table cellspacing="0">
        <tr>
           <th>Name</th>
           <th>Email</th>
           <th>Telefone</th>
           <th>Ação</th>
        </tr>

        <% users.forEach(function(user){ %>
          <tr>
              <td><%= user.name %></td>
              <td><%= user.email %></td>
              <td><%= user.phone %></td>
              <td><a href="edit/<%= user.id %>">Editar</a>  <a href="delete/<%= user.id %>">Deletar</a></td>
          </tr>
        <% })%>

      </table>
    </div>
  </div>
</div>
</body>
</html>
